{
  "name": "cryfs-web-backend",
  "version": "0.0.1",
  "description": "Backend API to make the cryfs.org website work",
  "main": "index.js",
  "author": "Sebastian Messmer",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "engineComment": "IMPORTANT: When updating node version, also update Globals.Function.Runtime in template.yaml",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "sam build",
    "local": "sam local start-api",
    "deploy": "sam build && sam deploy --no-confirm-changeset --no-fail-on-empty-changeset"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.712.0",
    "@aws-sdk/client-ssm": "^3.712.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "async-mutex": "^0.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^30.0.0",
    "@types/mailchimp__mailchimp_marketing": "^3.0.22",
    "@types/node": "^25.0.6",
    "esbuild": "^0.27.2",
    "eslint": "^9.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0"
  }
}
