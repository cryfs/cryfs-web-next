{
  "name": "cryfs-web-backend",
  "version": "0.0.1",
  "description": "Backend API to make the cryfs.org website work",
  "main": "index.js",
  "author": "Sebastian Messmer",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "engineComment": "IMPORTANT: When updating node version, also update Globals.Function.Runtime in template.yaml",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "build": "sam build",
    "local": "sam local start-api",
    "deploy": "sam build && sam deploy --no-confirm-changeset --no-fail-on-empty-changeset"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.712.0",
    "@aws-sdk/client-ssm": "^3.712.0",
    "async-mutex": "^0.5.0",
    "mailchimp-api-v3": "^1.15.0",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.5.0",
    "@types/md5": "^2.3.0",
    "@types/node": "^22.0.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.7.0"
  }
}
